<?xml version="1.0" encoding="utf-8"?>
 <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:app="http://schemas.android.com/apk/res-auto"
     xmlns:tools="http://schemas.android.com/tools"
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     android:autofillHints="-"
     android:background="#f7f7f7"
     app:circularflow_radiusInDP="20"
     tools:context=".MainActivity">

     <ImageView
         android:id="@+id/imV_logo1"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginStart="20dp"
         android:layout_marginTop="38dp"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="parent"
         app:srcCompat="@drawable/mt_logo"/>

     <ImageView
         android:id="@+id/imV_bell"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="38dp"
         android:layout_marginEnd="16dp"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintTop_toTopOf="parent"
         app:srcCompat="@drawable/bell" />

     <LinearLayout
         android:id="@+id/linearLayout1"
         android:layout_width="0dp"
         android:layout_height="200dp"
         android:layout_marginStart="1dp"
         android:layout_marginTop="90dp"
         android:layout_marginEnd="1dp"
         android:orientation="vertical"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="parent">

         <RelativeLayout
             android:id="@+id/post_relativeLayout2"
             android:layout_width="411dp"
             android:layout_height="200dp">

             <ImageView
                 android:id="@+id/imV_rbox"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 app:srcCompat="@drawable/rbox" />

             <ImageView
                 android:id="@+id/imV_profile"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 android:layout_marginStart="24dp"
                 android:layout_marginTop="20dp"
                 app:srcCompat="@drawable/profile" />

             <TextView
                 android:id="@+id/tV_name"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 android:layout_marginStart="96dp"
                 android:layout_marginTop="38dp"
                 android:fontFamily="@font/pretendardvariable_semibold"
                 android:letterSpacing="-0.03"
                 android:text="김모팁"
                 android:textColor="#111111"
                 android:textSize="20sp"
                 app:layout_constraintStart_toStartOf="parent"
                 app:layout_constraintTop_toTopOf="parent" />

             <ImageButton
                 android:id="@+id/imV_rightarrow1"
                 android:layout_width="48dp"
                 android:layout_height="48dp"
                 android:layout_alignParentTop="true"
                 android:layout_alignParentEnd="true"
                 android:layout_marginTop="36dp"
                 android:layout_marginEnd="16dp"
                 android:background="@android:color/transparent"
                 app:srcCompat="@drawable/right_arrow"
                 tools:ignore="SpeakableTextPresentCheck" />

             <ImageView
                 android:id="@+id/imV_clock"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 android:layout_marginStart="100dp"
                 android:layout_marginTop="110dp"
                 app:srcCompat="@drawable/clock" />

             <TextView
                 android:id="@+id/tV_time"
                 android:layout_width="100dp"
                 android:layout_height="20dp"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 android:layout_marginStart="66dp"
                 android:layout_marginTop="160dp"
                 android:background="@null"
                 android:ems="10"
                 android:fontFamily="@font/pretendardvariable_medium"
                 android:hint="0시간 0분"
                 android:letterSpacing="-0.03"
                 android:textAlignment="center"
                 android:textColor="#111111"
                 android:textSize="16sp"
                 app:layout_constraintStart_toStartOf="parent" />

             <ImageView
                 android:id="@+id/imV_prize"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignParentTop="true"
                 android:layout_alignParentEnd="true"
                 android:layout_marginTop="110dp"
                 android:layout_marginEnd="100dp"
                 app:srcCompat="@drawable/prize"
                 tools:ignore="ImageContrastCheck" />

             <TextView
                 android:id="@+id/tV_level"
                 android:layout_width="100dp"
                 android:layout_height="20dp"
                 android:layout_alignParentTop="true"
                 android:layout_alignParentEnd="true"
                 android:layout_marginTop="160dp"
                 android:layout_marginEnd="66dp"
                 android:fontFamily="@font/pretendardvariable_medium"
                 android:letterSpacing="-0.03"
                 android:text="LV.1"
                 android:textAlignment="center"
                 android:textColor="#111111"
                 android:textSize="16sp" />

             <View
                 android:id="@+id/divider_m"
                 android:layout_width="1dp"
                 android:layout_height="70dp"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 android:layout_alignParentEnd="true"
                 android:layout_marginStart="205dp"
                 android:layout_marginTop="100dp"
                 android:layout_marginEnd="205dp"
                 android:background="?android:attr/listDivider"
                 android:divider="#D9D9D9" />

         </RelativeLayout>
     </LinearLayout>

     <LinearLayout
         android:id="@+id/linearLayout2"
         android:layout_width="411dp"
         android:layout_height="290dp"
         android:layout_marginTop="320dp"
         android:orientation="vertical"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="parent">

         <RelativeLayout
             android:id="@+id/relativeLayout2"
             android:layout_width="411dp"
             android:layout_height="290dp">


             <ImageView
                 android:id="@+id/imV_box"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 app:srcCompat="@drawable/box" />

             <TextView
                 android:id="@+id/tV_cmcenter"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 android:layout_marginStart="24dp"
                 android:layout_marginTop="25dp"
                 android:fontFamily="@font/pretendardvariable_extrabold"
                 android:letterSpacing="-0.03"
                 android:text="고객센터"
                 android:textColor="#111111"
                 android:textSize="18sp" />

             <View
                 android:id="@+id/divider0"
                 android:layout_width="380dp"
                 android:layout_height="1dp"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 android:layout_alignParentEnd="true"
                 android:layout_marginStart="15dp"
                 android:layout_marginTop="60dp"
                 android:layout_marginEnd="15dp"
                 android:background="?android:attr/listDivider" />

             <RelativeLayout
                 android:id="@+id/relativeLayout2_1"
                 android:layout_width="411dp"
                 android:layout_height="45dp"
                 android:layout_alignParentTop="true"
                 android:layout_alignParentEnd="true"
                 android:layout_marginTop="60dp"
                 android:layout_marginEnd="0dp">


                 <ImageView
                     android:id="@+id/imV_miniarrow1"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_alignParentBottom="true"
                     android:layout_marginTop="12.5dp"
                     android:layout_marginEnd="16dp"
                     android:layout_marginBottom="12.5dp"
                     app:srcCompat="@drawable/miniarrow" />

                 <TextView
                     android:id="@+id/tV_notice"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentBottom="true"
                     android:layout_marginStart="24dp"
                     android:layout_marginTop="13dp"
                     android:layout_marginBottom="13dp"
                     android:fontFamily="@font/pretendardvariable_thin"
                     android:letterSpacing="-0.02"
                     android:lineSpacingExtra="8sp"
                     android:text="공지사항"
                     android:textColor="#111111"
                     android:textSize="14sp" />

                 <View
                     android:id="@+id/divider1"
                     android:layout_width="wrap_content"
                     android:layout_height="1dp"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_marginStart="15dp"
                     android:layout_marginTop="44dp"
                     android:layout_marginEnd="16dp"
                     android:background="?android:attr/listDivider"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintStart_toStartOf="parent" />

             </RelativeLayout>

             <RelativeLayout
                 android:id="@+id/relativeLayout2_2"
                 android:layout_width="411dp"
                 android:layout_height="45dp"
                 android:layout_below="@+id/relativeLayout2_1"
                 android:layout_alignParentEnd="true"
                 android:layout_marginTop="0dp"
                 android:layout_marginEnd="0dp">


                 <ImageView
                     android:id="@+id/imV_miniarrow2"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_marginTop="12.5dp"
                     android:layout_marginEnd="16dp"
                     app:srcCompat="@drawable/miniarrow" />

                 <TextView
                     android:id="@+id/tV_use"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentBottom="true"
                     android:layout_marginStart="24dp"
                     android:layout_marginTop="13dp"
                     android:layout_marginBottom="13dp"
                     android:fontFamily="@font/pretendardvariable_thin"
                     android:letterSpacing="-0.02"
                     android:lineSpacingExtra="8sp"
                     android:text="이용안내"
                     android:textColor="#111111"
                     android:textSize="14sp"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintTop_toTopOf="parent" />

                 <View
                     android:id="@+id/prog_divider1"
                     android:layout_width="380dp"
                     android:layout_height="1dp"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_marginStart="15dp"
                     android:layout_marginTop="44dp"
                     android:layout_marginEnd="15dp"
                     android:background="?android:attr/listDivider" />
             </RelativeLayout>

             <RelativeLayout
                 android:id="@+id/relativeLayout2_3"
                 android:layout_width="411dp"
                 android:layout_height="45dp"
                 android:layout_below="@+id/relativeLayout2_2"
                 android:layout_alignParentEnd="true"
                 android:layout_marginTop="0dp"
                 android:layout_marginEnd="0dp">


                 <ImageView
                     android:id="@+id/imV_miniarrow3"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_marginTop="12.5dp"
                     android:layout_marginEnd="16dp"
                     android:layout_marginBottom="16dp"
                     app:srcCompat="@drawable/miniarrow" />

                 <TextView
                     android:id="@+id/tV_faq"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentBottom="true"
                     android:layout_marginStart="24dp"
                     android:layout_marginBottom="13dp"
                     android:fontFamily="@font/pretendardvariable_thin"
                     android:letterSpacing="-0.02"
                     android:lineSpacingExtra="8sp"
                     android:text="FAQ"
                     android:textColor="#111111"
                     android:textSize="14sp" />

                 <View
                     android:id="@+id/divider3"
                     android:layout_width="380dp"
                     android:layout_height="1dp"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_marginStart="15dp"
                     android:layout_marginTop="44dp"
                     android:layout_marginEnd="16dp"
                     android:background="?android:attr/listDivider" />
             </RelativeLayout>

             <RelativeLayout
                 android:id="@+id/relativeLayout2_4"
                 android:layout_width="411dp"
                 android:layout_height="45dp"
                 android:layout_below="@+id/relativeLayout2_3"
                 android:layout_alignParentEnd="true"
                 android:layout_marginTop="0dp"
                 android:layout_marginEnd="0dp">


                 <ImageView
                     android:id="@+id/imV_miniarrow4"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_marginTop="12.5dp"
                     android:layout_marginEnd="16dp"
                     app:srcCompat="@drawable/miniarrow" />

                 <TextView
                     android:id="@+id/tV_ask"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentBottom="true"
                     android:layout_marginStart="24dp"
                     android:layout_marginBottom="13dp"
                     android:fontFamily="@font/pretendardvariable_thin"
                     android:letterSpacing="-0.02"
                     android:lineSpacingExtra="8sp"
                     android:text="문의하기"
                     android:textColor="#111111"
                     android:textSize="14sp" />

                 <View
                     android:id="@+id/prog_divider3"
                     android:layout_width="380dp"
                     android:layout_height="1dp"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_marginStart="15dp"
                     android:layout_marginTop="44dp"
                     android:layout_marginEnd="16dp"
                     android:background="?android:attr/listDivider" />
             </RelativeLayout>

             <RelativeLayout
                 android:id="@+id/relativeLayout2_5"
                 android:layout_width="411dp"
                 android:layout_height="45dp"
                 android:layout_below="@+id/relativeLayout2_4"
                 android:layout_alignParentEnd="true"
                 android:layout_marginTop="0dp"
                 android:layout_marginEnd="0dp">


                 <ImageView
                     android:id="@+id/imV_miniarrow5"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_marginTop="12.5dp"
                     android:layout_marginEnd="16dp"
                     app:srcCompat="@drawable/miniarrow" />

                 <TextView
                     android:id="@+id/tV_ver"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentBottom="true"
                     android:layout_marginStart="24dp"
                     android:layout_marginBottom="13dp"
                     android:fontFamily="@font/pretendardvariable_thin"
                     android:letterSpacing="-0.02"
                     android:lineSpacingExtra="8sp"
                     android:text="버전정보 v 1.0.9"
                     android:textColor="#111111"
                     android:textSize="14sp" />

             </RelativeLayout>

         </RelativeLayout>
     </LinearLayout>


 </androidx.constraintlayout.widget.ConstraintLayout>