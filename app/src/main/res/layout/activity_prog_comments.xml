<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".prog_comments">

    <TextView
        android:id="@+id/tV_club2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/pretendardvariable_semibold"
        android:letterSpacing="-0.03"
        android:text="클럽"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/cm_LinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="70dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:id="@+id/cm_relativeLayout1_1"
            android:layout_width="137dp"
            android:layout_height="50dp">

            <TextView
                android:id="@+id/tV_cmnotime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="25dp"
                android:layout_marginTop="14.5dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="14.5dp"
                android:fontFamily="@font/pretendardvariable_medium"
                android:letterSpacing="-0.08"
                android:text="NO TIME피드"
                android:textAlignment="center"
                android:textColor="#111111"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/cm_relativeLayout1_2"
            android:layout_width="137dp"
            android:layout_height="50dp">

            <TextView
                android:id="@+id/tV_cmdiary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="23.5dp"
                android:layout_marginTop="14.5dp"
                android:layout_marginEnd="23.5dp"
                android:layout_marginBottom="14.5dp"
                android:fontFamily="@font/pretendardvariable_medium"
                android:letterSpacing="-0.08"
                android:text="YES! 다이어리"
                android:textAlignment="center"
                android:textColor="#111111"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/cm_relativeLayout1_3"
            android:layout_width="137dp"
            android:layout_height="50dp">

            <TextView
                android:id="@+id/tV_cmprogot"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="30dp"
                android:layout_marginTop="14.5dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="14.5dp"
                android:fontFamily="@font/pretendardvariable_semibold"
                android:letterSpacing="-0.08"
                android:text="프로 갓생러"
                android:textAlignment="center"
                android:textColor="#fa4756"
                android:textSize="18sp" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/cm_LinearLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="93dp"
        android:background="#f7f7f7"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cm_LinearLayout1">

        <RelativeLayout
            android:id="@+id/cm_relativeLayout2_1"
            android:layout_width="411dp"
            android:layout_height="190dp">

            <ImageView
                android:id="@+id/imV_cmfeedbox1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="36dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="35dp"
                android:layout_marginBottom="20dp"
                app:srcCompat="@drawable/cm_ssfeedbox" />

            <TextView
                android:id="@+id/tV_cmhamo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/imV_cmfeedbox1"
                android:layout_marginStart="8dp"
                android:layout_marginTop="12dp"
                android:layout_toEndOf="@+id/imB_prohamo"
                android:fontFamily="@font/pretendardvariable_medium"
                android:letterSpacing="-0.08"
                android:text="하모0718"
                android:textColor="#29169F"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/tv_cmq1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/imV_cmfeedbox1"
                android:layout_marginStart="0dp"
                android:layout_marginTop="12dp"
                android:layout_toEndOf="@+id/tV_cmhamo"
                android:fontFamily="@font/pretendardvariable_medium"
                android:letterSpacing="-0.08"
                android:text="님의 질문"
                android:textColor="#797979"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/tV_exfield"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tV_cmhamo"
                android:layout_marginStart="8dp"
                android:layout_marginTop="0dp"
                android:layout_toEndOf="@+id/imB_prohamo"
                android:text="운동 분야"
                android:textColor="#797979"
                android:textSize="14sp"
                android:textStyle="normal" />

            <TextView
                android:id="@+id/tV_cmletter1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tV_exfield"
                android:layout_alignStart="@+id/imV_cmfeedbox1"
                android:layout_alignEnd="@+id/imV_cmfeedbox1"
                android:layout_marginStart="14dp"
                android:layout_marginTop="14dp"
                android:layout_marginEnd="14dp"
                android:fontFamily="@font/pretendardvariable_medium"
                android:text="Q. 안녕하세요. 요즘 헬스를 시작한 헬린이 입니다. 유산소가 먼저 인가요무산소가 먼저인가요? 그리고 초보인데 PT를 받아도 되나요? 멘토님의 의견이 궁금합니다. "
                android:textColor="#111111"
                android:textSize="12sp" />

            <ImageView
                android:id="@+id/imB_prohamo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imV_cmfeedbox1"
                android:layout_alignTop="@+id/imV_cmfeedbox1"
                android:layout_marginStart="16dp"
                android:layout_marginTop="15dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/pro_hamo"
                tools:ignore="SpeakableTextPresentCheck" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/cm_relativeLayout2_2"
            android:layout_width="411dp"
            android:layout_height="280dp">

            <ImageView
                android:id="@+id/imV_cmfeedbox2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="35dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="35dp"
                android:layout_marginBottom="30dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/cm_llfeedbox" />

            <ImageView
                android:id="@+id/imB_proex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imV_cmfeedbox2"
                android:layout_alignTop="@+id/imV_cmfeedbox2"
                android:layout_marginStart="16dp"
                android:layout_marginTop="15dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/pro_exercise" />

            <TextView
                android:id="@+id/tV_cmname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/imV_cmfeedbox2"
                android:layout_marginStart="8dp"
                android:layout_marginTop="12dp"
                android:layout_toEndOf="@+id/imB_proex"
                android:text="운동은 나의 삶"
                android:textColor="#29169F"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/tV_cmq2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/imV_cmfeedbox2"
                android:layout_marginStart="0dp"
                android:layout_marginTop="12dp"
                android:layout_toEndOf="@+id/tV_cmname"
                android:text="님의 답변"
                android:textColor="#797979"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/tV_divpro"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tV_cmname"
                android:layout_marginStart="8dp"
                android:layout_marginTop="0dp"
                android:layout_toEndOf="@+id/imB_proex"
                android:text="프로갓생러"
                android:textColor="#797979"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tV_cmletter2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tV_divpro"
                android:layout_alignStart="@+id/imV_cmfeedbox2"
                android:layout_alignEnd="@+id/imV_cmfeedbox2"
                android:layout_marginStart="14dp"
                android:layout_marginTop="14dp"
                android:layout_marginEnd="14dp"
                android:text="A. 헬스 초보라면 유산소를 먼저 가볍게 뛴 후 무산소를 해주는 게 좋습니다.  더 확실한 운동효과를 위해서는 워밍업 유산소와 마무 리 유산소 사이에 무산소를 해주는게 가장 효과적입니다. 초보이시다보니 무리하지 않는 선에서 가볍게 런닝머신을 뛰는  게 좋습니다."
                android:textColor="#111111"
                android:textSize="12sp" />

            <View
                android:id="@+id/cm_divider1"
                android:layout_width="344dp"
                android:layout_height="2dp"
                android:layout_alignStart="@+id/imV_cmfeedbox2"
                android:layout_alignEnd="@+id/imV_cmfeedbox2"
                android:layout_alignBottom="@+id/imV_cmfeedbox2"
                android:layout_marginStart="0dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="40dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <LinearLayout
                android:id="@+id/cm_LinearLayout3"
                android:layout_width="340dp"
                android:layout_height="40dp"
                android:layout_alignStart="@+id/imV_cmfeedbox2"
                android:layout_alignEnd="@+id/imV_cmfeedbox2"
                android:layout_alignBottom="@+id/imV_cmfeedbox2"
                android:layout_marginStart="0dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="0dp"
                android:orientation="horizontal">

                <RelativeLayout
                    android:id="@+id/pcm_relativeLayout3_1"
                    android:layout_width="100dp"
                    android:layout_height="40dp">

                    <ImageButton
                        android:id="@+id/imB_cmheart1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="14dp"
                        android:layout_marginBottom="10dp"
                        android:background="@android:color/transparent"
                        app:srcCompat="@drawable/heart"
                        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <TextView
                        android:id="@+id/tV_cmgood1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_toEndOf="@+id/imB_cmheart1"
                        android:fontFamily="@font/pretendardvariable_medium"
                        android:text="좋아요"
                        android:textColor="#c9c9c9" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/cm_relativeLayout3_2"
                    android:layout_width="150dp"
                    android:layout_height="40dp">

                    <ImageView
                        android:id="@+id/imB_cmcm1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="10dp"
                        android:layout_marginBottom="6dp"
                        app:srcCompat="@drawable/comment" />

                    <TextView
                        android:id="@+id/tV_cmcm1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="10dp"
                        android:layout_marginBottom="9dp"
                        android:layout_toEndOf="@+id/imB_cmcm1"
                        android:fontFamily="@font/pretendardvariable_medium"
                        android:text="답글"
                        android:textColor="#c9c9c9" />
                </RelativeLayout>
            </LinearLayout>

        </RelativeLayout>
    </LinearLayout>

    <ImageButton
        android:id="@+id/imB_cmredplus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="120dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/redplus"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>